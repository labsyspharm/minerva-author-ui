{
  "name": "minerva-author-ui",
  "version": "0.0.0",
  "description": "Minerva Author UI",
  "main": "demo.js",
  "scripts": {
    "start": "MINERVA_AUTHOR_ENV=local webpack-dev-server --host 0.0.0.0 --port 8487 --config ./webpack.config.js --mode development",
    "start:cloud": "MINERVA_AUTHOR_ENV=cloud webpack-dev-server --host 0.0.0.0 --port 8487 --config ./webpack.config.js --mode development",
    "build": "MINERVA_AUTHOR_ENV=local webpack --config ./webpack.config.js --progress --colors",
    "build:cloud": "MINERVA_AUTHOR_ENV=cloud webpack --config ./webpack.config.js --progress --colors",
    "deploy-demo": "aws s3 sync docs/ s3://minerva-author-ui-demo"
  },
  "keywords": [
    "openseadragon",
    "image processing",
    "microscopy",
    "webgl"
  ],
  "author": "Douglas Russell",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "node-sass": "^4.13.1",
    "react-hot-loader": "^3.1.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^0.21.0",
    "url-loader": "^4.0.0",
    "webpack": "^4.0.1",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "amazon-cognito-identity-js": "^3.2.1",
    "bootstrap": "4.1.0",
    "minerva-browser": "^2.8.0",
    "prop-types": "^15.6.1",
    "jquery": "3.5.1",
    "react": "^16.12.0",
    "react-color": "^2.14.1",
    "react-dom": "^16.12.0",
    "react-input-range": "^1.3.0",
    "react-select": "^3.0.8",
    "react-spinners": "^0.7.2",
    "reactcss": "^1.2.3",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "styled-components": "^5.1.0",
    "viawebgl": "git://github.com/thejohnhoffer/viaWebGL.git#webgl2"
  }
}
